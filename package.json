{
  "name": "eslint-config-valantic",
  "version": "16.0.1",
  "description": "The default ESLint config of valantic.",
  "email": "info@cec.valantic.com",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:js && npm run test:ts && npm run test:vue",
    "test:js": "eslint tests/tests.js --config index.js; exit 0",
    "test:ts": "eslint tests/tests.ts --config typescript.js; exit 0",
    "test:vue": "eslint tests/vue2.vue --config vue.js; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valantic/eslint-config-valantic.git"
  },
  "author": "valantic",
  "license": "MIT",
  "keywords": [
    "eslint",
    "valantic",
    "eslint-config"
  ],
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@types/node": "^20.4.5",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.0",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-vue": "^9.15.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 12"
  }
}
